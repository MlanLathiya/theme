            <span class="indicator"><i class="fas fa-plus"></i><i class="fas fa-minus"></i></span>
        </div>
        <div class="content">
            <p>
                {{ settings.next_day_uk_delivery }}
            </p>
        </div>
    </div>
    <div class="tab">
