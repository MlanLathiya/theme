<div class="delivery-options-accordian">
    <div class="tab">
        <div class="tab-head">
            <span class="label">Standard UK Delivery</span>
            <span class="indicator"><i class="fas fa-plus"></i><i class="fas fa-minus"></i></span>
        </div>
        <div class="content">
            <p>
                {{ settings.standard_uk_delivery }}
            </p>
        </div>
    </div>
    <div class="tab">
        <div class="tab-head">
            <span class="label">Express UK Delivery</span>
            <span class="indicator"><i class="fas fa-plus"></i><i class="fas fa-minus"></i></span>
        </div>
        <div class="content">
            <p>
                {{ settings.express_uk_delivery }}
