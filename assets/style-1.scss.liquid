/*=========================================
========== STAR RATING STYLES =============
=========================================*/

.star-rating-container {
  width: 100%;
  float: left;
}

fieldset,
label {
  margin: 0;
  padding: 0;
  display: block;
}

.rating {
  border: none;
  float: left;
}

.rating > input {
  display: none;
}
.rating > label:before {
  margin: 5px;
  font-size: 1.25em;
  font-family: FontAwesome;
  display: inline-block;
  content: "\f005";
}

.rating > .half:before {
  content: "\f089";
  position: absolute;
}

.rating > label {
  color: #ddd;
  float: right;
}

/***** Highlight Stars on Hover *****/

.rating > input:checked ~ label,
.rating:not(:checked) > label:hover,
.rating:not(:checked) > label:hover ~ label {
  color: #ffd700;
}

.rating > input:checked + label:hover,
.rating > input:checked ~ label:hover,
.rating > label:hover ~ input:checked ~ label,
.rating > input:checked ~ label:hover ~ label {
  color: #ffed85;
}

/*=========================================
========== ANIMATEDLOADER STYLES ==========
=========================================*/

.loader {
  display: none;
  min-width: 100px;
  align-items: center;
  justify-content: center;
  margin: auto;
  width: 100px;
  height: 100px;
  &.visible {
    display: flex;
  }
  .loader-bar {
    display: inline-block;
    width: 5px;
    height: 50px;
    border-radius: 5px;
    margin: 2px;
    animation: loading 1s ease-in-out infinite;
    background-color: $primary;
    &:nth-child(1) {
      animation-delay: 0;
    }
    &:nth-child(2) {
      animation-delay: 0.1s;
    }
    &:nth-child(3) {
      animation-delay: 0.2s;
    }
    &:nth-child(4) {
      animation-delay: 0.3s;
    }
  }
}

@keyframes loading {
  0% {
    transform: scale(1);
  }
  20% {
    transform: scale(1, 2);
  }
  40% {
    transform: scale(1);
  }
}

