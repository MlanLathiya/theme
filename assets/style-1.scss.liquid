@import url(//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css);

/*=========================================
============== VARIABLES ==================
=========================================*/

$defaultBreakPoint: 600px;

$sectionPadding: 30px;
$bottomMargin: 25px;

$primary: #000000;
$primaryRGB: rgb(0, 0, 0);
$secondary: #000000;

$grey: #1a1a1a;
$lightGrey: #ececec;
$darkGrey: #262626;
$error: #e60000;

$fontFamily: "Lato", sans-serif;
$cursiveFont: "Pacifico", cursive;

$bodyBackgroundColor: #fff;

$fontColor: #020202;
$greyText: #505050;

$border-color: #bcbcbd;

$bodyFontSize: 16px;
$h1Size: 48px;
$h2Size: 36px;
$h3Size: 28px;
$h4Size: 18px;
$h5Size: $bodyFontSize;
$h6Size: $bodyFontSize;

$bodyFontLineHeight: 1.5em;
$h1LineHeight: 1.5em;
$h2LineHeight: 1.5em;
$h3LineHeight: 1.5em;
$h4LineHeight: 1.5em;
$h5LineHeight: 1.5em;
$h6LineHeight: 1.5em;

@media (max-width: $defaultBreakPoint) {
  $bodyFontSize: 16px;
  $h1Size: 32px;
  $h2Size: 26px;
  $h3Size: 22px;
  $h4Size: 18px;
  $h5Size: $bodyFontSize;
  $h6Size: $bodyFontSize;
}


// CMS Padding, Margin
// Min Breakpoints
$breakpoint-smallest: "only screen and (min-width: 27em)"; // > 448px
$breakpoint-small: "only screen and (min-width:30em)"; // > 480px
$breakpoint-medium: "only screen and (min-width:48em)"; // > 768px
$breakpoint-wide: "only screen and (min-width:64em)"; // > 1024px
$breakpoint-wider: "only screen and (min-width:80em)"; // > 1280px
$breakpoint-widest: "only screen and (min-width:94em)"; // > 1504px
$breakpoint-content-wide: "only screen and (min-width:98.125em)"; // > 1570px


// Max Breakpoints
$breakpoint-max-smallest: "only screen and (max-width: 26.99em)"; // < 448px
$breakpoint-max-small: "only screen and (max-width:29.99em)"; // < 480px
$breakpoint-max-medium: "only screen and (max-width:47.99em)"; // < 768px
$breakpoint-max-wide: "only screen and (max-width:63.99em)"; // > 1024px

$cms-padding-top: 40px;
$cms-padding-bottom: 40px;
$cms-margin-top: 40px;
$cms-margin-bottom: 40px;

$cms-padding-top-mobile: 25px;
$cms-padding-bottom-mobile: 25px;
$cms-margin-top-mobile: 25px;
$cms-margin-bottom-mobile: 25px;

$cms-column-padding: 10px;
$banner-caption-located-image-button-bottom: 30px;
$banner-caption-located-image-button-top: 30px;
$banner-caption-located-image-button-bottom-mobile: 20px;
$banner-caption-located-image-button-top-mobile: 20px;

/*=========================================
============== Color Swatches ==================
=========================================*/
.color-swatch {
  &#beige {
    background-color: #f5f5dc;
  }
  &#blue {
    background-color: #1e90ff;
  }
  &#black {
    background-color: #000000;
  }
  &#brown {
    background-color: #8b4513;
  }
  &#green {
    background-color: $primary;
  }
  &#grey {
    background-color: #a9a9a9;
  }
  &#metallic {
    background: #c0c0c0;
    background: -moz-linear-gradient(
      left,
      #c0c0c0 0%,
      #ffffff 50%,
      #c0c0c0 100%
    );
    background: -webkit-linear-gradient(
      left,
      #c0c0c0 0%,
      #ffffff 50%,
      #c0c0c0 100%
    );
    background: linear-gradient(
      to right,
      #c0c0c0 0%,
      #ffffff 50%,
      #c0c0c0 100%
    );
  }
  &#multi {
    background: linear-gradient(
      to right,
      #e60000,
      #ffa500,
      #ffff00,
      $primary,
      #1e90ff,
      #ff69b4,
      #663399
    );
  }
  &#orange {
    background-color: #ffa500;
  }
  &#pink {
    background-color: #ff69b4;
  }
  &#purple {
    background-color: #663399;
  }
  &#red {
    background-color: #e60000;
  }
  &#white {
    background-color: #fff;
  }
  &#yellow {
    background-color: #ffff00;
  }
}

.BOLD-mc-picker-mnt {
  display: none !important;
}

/********
TYPORGRAPHY
*********/

h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  margin-bottom: $bottomMargin;
  font-weight: 750;
}
h1 {
  font-size: $h1Size;
  line-height: $h1LineHeight;
}
h2 {
  font-size: $h2Size;
  line-height: $h2LineHeight;
}
h3 {
  font-size: $h3Size;
  line-height: $h3LineHeight;
}
h4 {
  font-size: $h4Size;
  line-height: $h4LineHeight;
}
h5 {
  font-size: $h5Size;
  line-height: $h5LineHeight;
}
h6 {
  font-size: $h6Size;
  line-height: $h6LineHeight;
}

.no-max-width {
  * {
    max-width: 100%;
  }
}

.cursive-font {
  font-family: $cursiveFont;
}

.text-center {
  text-align: center;
}

.bold {
  font-weight: bold;
}

.underlined {
  text-decoration: underline;
}

.uppercase {
  text-transform: uppercase;
}

/********
LISTS
*********/

ul.unstyled {
  list-style-type: none;
  margin: 0;
  padding: 0;
}

.inline {
  li {
    display: inline;
  }
}

/********
IMAGES
*********/

.img-center {
  display: block;
  margin-left: auto;
  margin-right: auto;
}

.has-background-image {
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
}

/********
LAYOUTS
*********/

.row {
  width: 100%;
}

.flex-grid {
  width: 100%;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  flex-wrap: wrap;
  // overflow: hidden;
  .col {
    flex: 1;
    &.col-1 {
      flex-basis: 8.33%;
      max-width: 8.33%;
    }
    &.col-2 {
      flex-basis: 16.66%;
      max-width: 16.66%;
    }
    &.col-2-custom {
      flex-basis: 20%;
      max-width: 20%;
    }
    &.col-3 {
      flex-basis: 25%;
      max-width: 25%;
    }
    &.col-4 {
      flex-basis: 33.33%;
      max-width: 33.33%;
    }
    &.col-5 {
      flex-basis: 41.66%;
      max-width: 41.66%;
    }
    &.col-6 {
      flex-basis: 50%;
      max-width: 50%;
    }
    &.col-7 {
      flex-basis: 58.33%;
      max-width: 58.33%;
    }
    &.col-8 {
      flex-basis: 66.66%;
      max-width: 66.66%;
    }
    &.col-9 {
      flex-basis: 75%;
      max-width: 75%;
    }
    &.col-10 {
      flex-basis: 83.33%;
      max-width: 83.33%;
    }
    &.col-11 {
      flex-basis: 91.66%;
      max-width: 91.66%;
    }
    &.col-12 {
      flex-basis: 100%;
      max-width: 100%;
    }
    &.vertical-center {
      align-self: center;
    }
  }
}

.margin-centered {
  margin: 0 auto;
}

.center-this {
  display: flex;
  justify-content: center;
}

.relative {
  position: relative;
}

/********
BUTTONS
*********/

.btn {
  // min-width: 200px;
  // min-height: 50px;
  padding: 10px 15px;
  border: none;
  transition: all linear 0.3s;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  margin-bottom: 10px;
  margin-right: 10px;
  font-size: 16px;
  line-height: 1.5em;
  text-transform: uppercase;
  cursor: pointer;
  &.btn-primary {
    color: $primary;
    border: 1px solid $primary;
    &:hover {
      color: #fff;
      background-color: $primary;
    }
  }
  &.btn-secondary {
    background-color: $secondary;
    &:hover {
      background-color: darken($secondary, 10%);
      color: #fff;
    }
  }
}

#add-to-cart {
  border-radius: 0;
  border: 1px solid $primary;
}

/********
FORMS
*********/

form {
  position: relative;
  width: 100%;
  margin-left: auto;
  input,
  select,
  textarea {
    width: 100%;
    outline: none;
    border: none;
    border-bottom: 1px solid $primary;
    min-height: 30px;
    padding: 15px;
    &:focus {
      outline: none;
    }
  }
}

.errors {
  width: 100%;
}

#optin-email-input {
  display: flex;
  display: none;
  justify-content: center;
  padding: 15px;
  label {
    width: auto;
    background: none;
    color: $greyText;
  }
  input[type="email"] {
    min-width: 280px;
    border-bottom: 1px solid $primary;
  }
}

#contact-agree {
  margin-right: 5px;
}

#contact-decline {
  margin-left: 5px;
}

#contact-agree,
#contact-decline {
  background-color: #fff;
  border: 1px solid $primary;
  overflow: auto;
  width: 150px;
  height: 50px;
  margin-bottom: 15px;
  label {
    width: 100%;
    height: 100%;
    background: transparent;
    color: $primary;
    padding: 0px;
    span {
      text-align: center;
      padding: 3px 0px;
      display: block;
      width: 100%;
      height: 100%;
      display: flex;
      align-items: center;
      justify-content: center;
      transition: all linear 0.3s;
      &:hover {
        background-color: $primary;
        color: #fff;
      }
    }
    input {
      position: relative;
      cursor: pointer;
      display: none;
      &:checked + span {
        background-color: $primary;
        color: #fff;
      }
    }
  }
}

/********
TABLES
*********/

table {
  width: 100%;
  tr {
    &:first-child {
      background-color: $primary !important;
      color: #fff;
    }
    &:nth-child(odd) {
      background-color: #fafafa;
    }
    &:nth-child(even) {
      background-color: $lightGrey;
    }
    td {
      padding: 5px;
    }
  }
}

// Fix for Bazaar Voice.
table tr.bv-section-summary-inline,
table tr.bv-inline-histogram-ratings-star-container,
table tr.bv-secondary-rating-summary-stars {
  background-color: #fafafa !important;
}

/*=========================================
========== CUSTOM INPUTS ==================
=========================================*/
.quantity-selector {
  width: 90px;
  display: flex;
  border: 1px solid $lightGrey;
  div {
    display: flex;
    align-items: center;
    justify-content: center;
    flex: 1;
    height: 30px;
    span {
      -webkit-user-select: none; /* Safari */
      -moz-user-select: none; /* Firefox */
      -ms-user-select: none; /* IE10+/Edge */
      user-select: none; /* Standard */
    }
    &.action {
      cursor: pointer;
      background-color: $lightGrey;
    }
  }
  &.product-page {
    margin: 10px 0;
    border: 1px solid $primary;
    div {
      background-color: #fff;
      &.action {
        background-color: $primary;
        color: #fff;
      }
    }
  }
}

/*=========================================
========== SEARCH MODAL STYLES ============
=========================================*/

#search-modal {
  width: 100%;
  height: 100vh;
  display: flex;
  visibility: hidden;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  background: rgba($primaryRGB, 0.8);
  position: fixed;
  top: 0;
  left: 0;
  opacity: 0;
  z-index: 999999;
  transition: opacity linear 0.3s;
  #search-modal-close {
    position: absolute;
    top: 50px;
    right: 50px;
    color: #fff;
    font-size: 40px;
    &:hover {
      cursor: pointer;
    }
  }
  input {
    color: #fff;
    background: none;
    border: none;
    border-bottom: 1px solid #fff;
    font-size: 30px;
    font-weight: 900;
    padding: 6px 15px;
    height: auto;
    &::placeholder {
      color: #fff;
      opacity: 1;
    }
    &:moz-placeholder {
      color: #fff;
      opacity: 1;
    }
    &::moz-placeholder {
      color: #fff;
      opacity: 1;
    }
    &:-ms-input-placeholder {
      color: #fff;
      opacity: 1;
    }
    &:focus {
      outline: none;
    }
  }
  &.is-open {
    opacity: 1;
    visibility: visible;
  }
}

/*=========================================
============ HEADER STYLES ================
=========================================*/

.telephone-banner {
  background: $lightGrey;
  text-align: right;
}

header {
  width: 100%;
  padding: 10px 30px;
  // .container {
  //   margin: 0 auto;
  //   width: 100%;
  //   max-width: 1300px;
  // }
  section {
    @media (max-width: 780px) {
      padding: 0;
    }
  }
  .flex-grid {
    @media (max-width: 800px) {
      display: block;
      .col {
        // width: 100%;
        max-width: 100% !important;
        flex-basis: 100% !important;
      }
    }
  }
  .logo-container {
    padding: $sectionPadding/2 $sectionPadding $sectionPadding/2 0px;
    @media (max-width: 800px) {
      padding: $sectionPadding/2 $sectionPadding;
    }
    a {
      display: block;
      width: 100%;
      height: 100%;
      display: flex;
      align-items: center;
      justify-content: start;
    }
    img {
      width: 100%;
      height: auto;
      max-width: 360px;
      display: block;
      @media (max-width: 800px) {
        margin: 0 auto;
      }
    }
  }
  .actions-container {
    padding: $sectionPadding/2 0px $sectionPadding/2 $sectionPadding;
    @media (max-width: 1000px) {
      padding: 15px;
    }
    @media (max-width: 550px) {
      padding: 0;
    }
    ul {
      width: 100%;
      text-align: center;
      display: flex;
      align-items: center;
      justify-content: flex-end;
      padding-bottom: 20px;
      @media (max-width: 800px) {
        justify-content: center;
      }
      li {
        &.hidden-on-mobile {
          @media (max-width: 780px) {
            display: none;
          }
        }
        &.currency-li {
          .placeholder {
            position: relative;
            display: block;
            width: 100%;
            height: 100%;
            background-color: $primary;
            color: #fff;
            z-index: 1;
            border-radius: 2px;
            cursor: pointer;
            padding: 5px $sectionPadding/2;
          }
          .select {
            // margin-top: $bottomMargin;
            position: absolute;
            top: 100%;
            left: 7px;
            display: none;
            width: 100px;
            color: #cccccc;
            vertical-align: middle;
            text-align: left;
            z-index: 10;
            transition: all linear 0.3s;
            &.is-active {
              display: block;
            }
            ul {
              overflow: hidden;
              overflow-y: auto;
              width: 100%;
              z-index: 100;
              background-color: #fff;
              display: block;
              color: $fontColor;
              padding: 0;
              border: 1px solid $primary;
              li {
                display: block;
                text-align: center;
                padding: 5px 10px;
                cursor: pointer;
                margin: 0;
                transition: all linear 0.3s;
                &:hover {
                  background: $primary;
                  color: #fff;
                }
              }
            }
          }
        }
        &:not(:last-child) {
          margin-right: 15px;
        }
        a {
          text-transform: uppercase;
          font-weight: 500;
        }
        &#cart {
          font-size: 30px;
          position: relative;
          a {
            &:after {
              content: attr(data-after);
              position: absolute;
              top: -10px;
              right: -10px;
              width: 20px;
              height: 20px;
              border-radius: 50%;
              background: $primary;
              color: #fff;
              display: flex;
              align-items: center;
              justify-content: center;
              text-align: center;
              font-size: 10px;
            }
          }
          &.empty {
            a:after {
              display: none;
            }
          }
        }
      }
    }
    #search-box {
      position: relative;
      width: 100%;
      max-width: 478px;
      margin-left: auto;
      margin-bottom: 2px; // Added for Explorer
      @media (max-width: 770px) {
        display: flex;
        align-items: center;
      }
      #open-nav {
        background: transparent;
        position: relative;
        height: 50px;
        width: 50px;
        cursor: pointer;
        display: none;
        @media (max-width: 780px) {
          display: block;
        }
        span,
        span:before,
        span:after {
          cursor: pointer;
          border-radius: 1px;
          height: 6px;
          width: 35px;
          background: $fontColor;
          position: absolute;
          left: 0;
          top: 50%;
          display: block;
          content: "";
          transition: all 0.5s ease-in-out;
        }
        span {
          &:before {
            top: -10px;
          }
          &:after {
            top: 10px;
          }
        }
        &.active {
          span {
            background-color: transparent;
            &:before {
              top: 0;
              transform: rotate(135deg);
            }
            &:after {
              top: 0;
              transform: rotate(-135deg);
            }
          }
        }
      }
      form {
        border: 1px solid $primary;
        @media (max-width: 770px) {
          width: 85%;
          max-width: 405px;
        }
      }
      input[type="text"] {
        width: 100%;
        outline: none;
        border: none;
        height: 30px;
        padding: 15px;
        padding-left: 15%;
        &::placeholder {
          text-align: right;
        }
        &:moz-placeholder {
          text-align: right;
        }
        &::moz-placeholder {
          text-align: right;
        }
        &:-ms-input-placeholder {
          text-align: right;
        }
        &:focus {
          outline: none;
        }
      }
      #search-button {
        position: absolute;
        top: 0;
        left: 0;
        width: 10%;
        height: 100%;
        border: none;
        border-right: 1px solid $primary;
        font-size: 14px;
        color: $primary;
        text-align: center;
        cursor: pointer;
        background-color: #fff;
      }
      @media (max-width: 800px) {
        margin: 0 auto;
      }
    }
  }
  .nav-section {
    padding: 5px 30px;
    .nav-container {
      nav {
        width: 100%;
        @media (max-width: 780px) {
          display: none;
        }
        ul {
          text-align: center;
          display: flex;
          justify-content: space-between;
          li {
            position: relative;
            &:not(:last-child) {
              margin-right: 15px;
            }
            &:hover {
              .child {
                opacity: 1;
                visibility: visible;
              }
            }
            a {
              text-transform: uppercase;
              font-weight: 500;
              position: relative;
              &:after {
                content: "";
                position: absolute;
                top: 100%;
                left: 0;
                display: block;
                width: 0;
                height: 2px;
                background-color: $primary;
                transition: all linear 0.3s;
              }
              &:hover {
                &:after {
                  width: 100%;
                }
              }
            }
            .child {
              min-width: 250px;
              padding: $sectionPadding/2;
              background: #fff;
              display: block;
              text-align: left;
              border: 1px solid $lightGrey;
              position: absolute;
              left: 0;
              z-index: 999;
              opacity: 0;
              visibility: hidden;
              transition: all linear 0.3s;
              li {
                display: block;
                &:not(:last-child) {
                  margin-bottom: 15px;
                }
              }
            }
          }
        }
      }
    }
  }
}

.mobile-nav {
  width: 100%;
  background-color: $primary;
  position: absolute;
  top: 100%;
  left: 0;
  z-index: 999;
  visibility: hidden;
  opacity: 0;
  transition: all linear 0.4s;
  padding: 0; //$sectionPadding 0;
  @media (min-width: 781px) {
    display: none !important;
  }
  &.is-open {
    visibility: visible;
    opacity: 1;
  }
  nav {
    width: 100%;
    ul {
      li {
        border-bottom: 1px solid #546966;
        a {
          text-align: center;
          color: #fff;
          width: 100%;
          height: 100%;
          padding: $sectionPadding/2 $sectionPadding;
          display: block;
          transition: all linear 0.3s;
          &:hover {
            background-color: $secondary;
          }
        }
        &:last-child {
          border: none;
        }
        .child {
          display: none;
          padding: 0;
          background-color: $secondary;
          li {
            &:hover {
              background-color: $primary;
            }
          }
        }
        &.has-children {
          position: relative;
          &::after {
            content: '+';
            position: absolute;
            right: 20px;
            display: block;
            color: #fff;
            top: 50%;
            transform: translateY(-50%);
          }
        }
        &.open-child {
          background-color: $secondary;
          &::after {
            content: '';
          }
          .child {
            display: block;
          }
        }
      }
    }
  }
}

#back-to-top-btn {
  position: fixed;
  bottom: 25px;
  right: 25px;
  z-index: 10;
  display: block;
  width: 50px;
  height: 50px;
  background-color: $primary;
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 0;
  visibility: hidden;
  transition: all linear 0.3s;
  i,
  svg {
    color: #fff;
    font-size: 24px;
    transition: all linear 0.3s;
  }
  &:hover {
    background-color: lighten($primary, 10%);
    i,
    svg {
      transform: translateY(-5px);
    }
  }
  &.is-visible {
    visibility: visible;
    opacity: 1;
    cursor: pointer;
  }
}

/*=========================================
============ USP BAR STYLES ===============
=========================================*/

.usp-bar {
  // background-color: $lightGrey;
  color: $primary;
  padding: $sectionPadding/2 !important;
  margin-top: 15px;
  margin-bottom: 15px;
  background-color: #efefef;
  .col {
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 18px;
    &:not(:last-child) {
