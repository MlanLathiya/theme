/*=========================================
============ PRICE SLIDER STYLES ==========
=========================================*/

.range-slider {
  margin: auto;
  text-align: center;
  position: relative;
  height: 48px;
  #min,
  #max {
    position: absolute;
    top: 0;
    font-size: 12px;
  }
  #min {
    left: 0;
  }
  #max {
    right: 0;
  }
  #current-range {
    position: absolute;
    bottom: -5px;
    left: 50%;
    transform: translateX(-50%);
    font-size: 12px;
    width: 100%;
  }
  input[type="number"] {
    text-align: center;
    -moz-appearance: textfield;
    border: none;
    width: 36px;
    padding: 0;
  }
  input[type="number"]::-webkit-outer-spin-button,
  input[type="number"]::-webkit-inner-spin-button {
    -webkit-appearance: none;
  }
  input[type="number"]:invalid,
  input[type="number"]:out-of-range {
    border: 1px solid #ff6347;
  }
  input[type="range"] {
    -webkit-appearance: none;
    -moz-appearance: none;
    width: 100%;
    position: absolute;
    left: 0;
    top: 21px;
    &:nth-of-type(2) {
      height: 0;
      top: 27px;
      &::-moz-range-track {
        height: 0px;
        pointer-events: none;
      }
      &::-webkit-slider-runnable-track {
        height: 0;
      }
      &::-webkit-slider-thumb {
        margin-top: -12px;
      }
    }
  }
  input[type="range"]:focus {
    outline: none;
  }
  input[type="range"]:focus::-webkit-slider-runnable-track {
    background: $primary;
  }
  input[type="range"]:focus::-ms-fill-lower {
    background: $primary;
  }
  input[type="range"]:focus::-ms-fill-upper {
    background: $primary;
  }
  input[type="range"]::-webkit-slider-runnable-track {
    width: 100%;
    height: 5px;
    cursor: pointer;
    animate: 0.2s;
    background: $primary;
    box-shadow: none;
    border: 0;
  }
  input[type="range"]::-webkit-slider-thumb {
    z-index: 2;
    position: relative;
    box-shadow: 0px 0px 0px #000;
    border: 1px solid $primary;
    height: 18px;
    width: 18px;
    border-radius: 25px;
    background: lighten($primary, 20%);
    cursor: pointer;
    appearance: none;
    -webkit-appearance: none;
    margin-top: -7px;
  }
  input[type="range"]::-moz-range-track {
    width: 100%;
    height: 5px;
    cursor: pointer;
    animate: 0.2s;
    background: $primary;
    border-radius: 1px;
    box-shadow: none;
    border: 0;
    z-index: -1;
    position: relative;
  }
  input[type="range"]::-moz-range-thumb {
    z-index: 2;
    position: relative;
    box-shadow: 0px 0px 0px #000;
    border: 1px solid $primary;
    height: 18px;
    width: 18px;
    border-radius: 25px;
    background: lighten($primary, 20%);
    cursor: pointer;
  }
  input[type="range"]::-ms-track {
    width: 100%;
    height: 5px;
    cursor: pointer;
    animate: 0.2s;
    background: transparent;
    border-color: transparent;
    color: transparent;
  }
  input[type="range"]::-ms-fill-lower,
  input[type="range"]::-ms-fill-upper {
    background: $primary;
