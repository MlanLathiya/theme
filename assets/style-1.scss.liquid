@keyframes ticker{
  0%{
    -webkit-transform:translate3d(0, 0, 0);
    transform:translate3d(0, 0, 0)
  }
  100%{
    -webkit-transform:translate3d(-100%, 0, 0);
    transform:translate3d(-100%, 0, 0)
  }
}

.ticker-section {
  padding-top: 0;
  padding-bottom: 0;
}
.ticker {
	position: relative;
	font-weight: 750;
	font-size: 22px;
	z-index: 1;
	.ticker__inner {
		overflow: hidden;
		white-space: nowrap;
		.tick {
			display: block;
			color: transparent;
      margin-right: 20px;
      p{
        font-size: inherit;
        max-width: unset;
        margin-bottom: 0;
      }
			//-webkit-text-stroke: 1px $text-dark;
      color: #111;
      
      strong{
        color: #f46a3b;
        font-weight: inherit;
      }
		}

		.ticks {
			display: table-cell;
			-webkit-animation-iteration-count: infinite;
			animation-iteration-count: infinite;
			-webkit-animation-timing-function: linear;
			animation-timing-function: linear;
			-webkit-animation-name: ticker;
			animation-name: ticker;			
      -webkit-animation-duration: 12s;
			animation-duration: 12s;
      

      @media screen and (max-width: 767px){
        -webkit-animation-duration: 15s;
        animation-duration: 15s;
      }

			&.active {
				.tick {
					color: #111;
				}
			}
		}
	}

  &.stroke{
    .ticker__inner {     
      .tick {        
        -webkit-text-stroke: 1px #111;
      }
      .ticks {       
  
        &.active {
          .tick {
            color: transparent;
          }
        }
      }
    }
  }

	@media screen and (max-width: 767px){
		font-size: 24px;
    padding: 0;
	}

	@media screen and (min-width: 979px){
		font-size: 32px;
	}
}

section.collections{
  .flex-grid{
    margin-left: -15px;
    margin-right: -15px;
    width: auto;
  }
}

footer{
  @media screen and (max-width: 1360px){
    padding-left: 30px;
    padding-right: 30px;
  }
  .flex-grid{
    margin-left: -15px;
      margin-right: -15px;
      width: auto;
  }
}

.collection-swatches-holder{
  .color-swatch{
    &:not(:last-child){
      margin-right: 5px;
    }
    display: inline-block;
    width: 22px;
    height: 22px;
    margin-bottom: 10px;
    border-radius: 50%;
    position: relative;
    background-position: 67% 70%;
    cursor: pointer;
    border: 1px solid transparent;
    &:hover {
      .tooltiptext{
        visibility: visible;
      }        
    }
    &:after{
      border: 1px solid transparent;
      content: "";
      position: absolute;
      left: 50%;
      top: 50%;
      width: 24px;
      height: 24px;
      transform: translate(-50%, -50%);
      border-radius: 50%;
    }
    &.active{
      &:after{
        border-color: #000;
      }      
    }
  }
  .tooltiptext {
      visibility: hidden;
      background-color: #000;
      color: #fff;
      text-align: center;
      padding: 5px 10px;
      border-radius: 6px;
      position: absolute;
      z-index: 1;
      bottom: 100%;
      left: 50%;
      transform: translateX(-50%);
      width: 85px;
      font-size: 12px;
  }
}